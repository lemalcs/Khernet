<UserControl x:Class="Khernet.UI.Controls.PageContainer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Khernet.UI.Controls"
             xmlns:depend="clr-namespace:Khernet.UI.DependencyProperties"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300"
             Background="Transparent">
    <Grid>
        <Frame Name="OldFrame"
               depend:DisableFrameHistoryProperty.Value="True"/>
        <Frame Name="NewFrame"
               depend:DisableFrameHistoryProperty.Value="True"/>
    </Grid>
</UserControl>
